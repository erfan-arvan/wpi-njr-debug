    private void removeMoverRecordAt( @org.checkerframework.checker.initialization.qual.Initialized @org.checkerframework.checker.nullness.qual.NonNull int idx) {
        this.turnMoves.remove(idx);
        this.turnMoves.add(new MoveRecord());
        this.turnMovesIdx--;
    }

